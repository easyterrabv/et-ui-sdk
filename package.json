  "scripts": {
    "test": "npx cypress run --component --headless",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,vue}\"",
    "formatHook": "prettier",
    "lint": "tslint -p tsconfig.json",
    "type-check": "vue-tsc --skipLibCheck --noEmit -p tsconfig.json --composite false",
    "postinstall": "node -e \"try { require('child_process').execSync('./install-hooks.sh'); } catch (e) {}\"",
    "everything": "npm install && npm run format && npm run lint && npm run type-check && npm run test && npm run preview"
  },
  "engines": {
    "node": ">=22.17"
  },
